<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ui_background"
    tools:context="com.sdot.yidai.ui.ApplyDateListActivity">

    <include layout="@layout/title_layout"></include>

    <LinearLayout
        android:layout_marginTop="1dp"
        android:layout_below="@+id/title_re_layout"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/suijie_layout"
            android:clickable="true"
            android:background="@drawable/linear_click_selector"
            android:layout_below="@+id/title_re_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:id="@+id/suiji"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="28dp"
                android:text="随借随还" />

            <ImageView
                android:id="@+id/suiji_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="26dp"
                android:layout_marginRight="26dp"
                android:src="@drawable/right_jiantou" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/wangdian_layout"
            android:layout_marginTop="1dp"
            android:clickable="true"
            android:background="@drawable/linear_click_selector"
            android:layout_below="@+id/title_re_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:id="@+id/wangdian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="28dp"
                android:text="网点信用贷" />

            <ImageView
                android:id="@+id/wangdian_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="26dp"
                android:layout_marginRight="26dp"
                android:src="@drawable/right_jiantou" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rongzi_layout"
            android:layout_marginTop="1dp"
            android:clickable="true"
            android:background="@drawable/linear_click_selector"
            android:layout_below="@+id/title_re_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="28dp"
                android:text="融资租赁" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="26dp"
                android:layout_marginRight="26dp"
                android:src="@drawable/right_jiantou" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/miandan_layout"
            android:layout_marginTop="1dp"
            android:clickable="true"
            android:background="@drawable/linear_click_selector"
            android:layout_below="@+id/title_re_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:paddingTop="15dp"
                android:paddingBottom="15dp"
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="28dp"
                android:text="面单白条" />

            <ImageView
                android:id="@+id/imageView56"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="26dp"
                android:layout_marginRight="26dp"
                android:src="@drawable/right_jiantou" />
        </RelativeLayout>




    </LinearLayout>



</RelativeLayout>
